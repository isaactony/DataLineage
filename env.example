# Airflow Configuration (optional)
AIRFLOW_HOME=./lineage/airflow
AIRFLOW_WEBSERVER_PORT=8080

# OpenLineage Configuration
OPENLINEAGE_URL=http://localhost:5002
OPENLINEAGE_NAMESPACE=data-lineage-audit
